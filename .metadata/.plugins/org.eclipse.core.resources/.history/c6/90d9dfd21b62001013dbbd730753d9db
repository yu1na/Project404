spring.application.name=Project404_yuna

#포트설정
server.port=8081

#JSP설정
spring.mvc.view.prefix=/WEB-INF/views/
spring.mvc.view.suffix=.jsp

# oracle 접속정보 설정
spring.datasource.driver-class-name=oracle.jdbc.OracleDriver
spring.datasource.url=jdbc:oracle:thin:@localhost:1521:xe
spring.datasource.username=AIR_GWS
spring.datasource.password=1234

spring.jpa.hibernate.ddl-auto=update

# 실행되는 SQL 쿼리를 콘솔에 출력할지 여부
spring.jpa.show-sql=true

# SQL 쿼리를 더 읽기 쉽게 포맷팅할지 여부
spring.jpa.properties.hibernate.format_sql=true

# 사용하는 데이터베이스 방언(dialect) 설정
# Oracle 11g 이후 버전에서는 주로 Oracle12cDialect를 사용합니다.
# 만약 오류가 발생하면 OracleDialect, Oracle9iDialect, Oracle10gDialect 등을 시도해볼 수 있습니다.
spring.jpa.database-platform=org.hibernate.dialect.OracleDialect

spring.web.resources.static-locations=classpath:/static/,classpath:/resources/,classpath:/META-INF/resources/